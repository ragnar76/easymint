> PROCEDURE user_on_open
  '
  ' This procedure is called when the program is run, after the RSC is
  ' loaded and just before the main loop. You can open program windows,
  ' toolboxes etc. here, or  init things for your program like
  ' loading an *.INF or .DAT file.
  '
  ' If run as an accessory, this procedure is called EVERY TIME
  ' THE ACCESSORY IS OPENED. If you need to do anything just ONCE,
  ' like disable menu-entries spesific to PROGRAM execution, set a global
  ' flag here to avoid doing things EVERY time the accessory is opened.
  @my_init
  '
  ' ÍÍwsnippetÍÍ  - Wrinkle-Code: (dieses Flag nicht lîschen oder verÑndern)
  '
  '
  '
  '
  '  fV-Update: 
  ' Der folgende Wrinkle-Code ist durch das Update hinzugekommen:
  '  @messagewin_open(" Messagewindow ",50,200,20,400,180,iconify_icon_1&)   !ÍÍFVW:messagewinÍÍ
  '  fV-Update 
  '
  ' ÍÍwsnippetÍÍ  - Ende des Wrinkle-Codes: (dieses Flag nicht lîschen oder verÑndern)
  '
  IF @exist(install_path$+"\tmp\position.tmp",63)<>0
    @file_check
    IF exit_program!=FALSE
      @info0
    ENDIF
  ENDIF
  IF @exist(install_path$+"\tmp\position.tmp",63)=0
    @make_ext2
  ENDIF
  '
  '
  '
RETURN
> PROCEDURE user_rsc_interact(index&,tree&,object&,mc&,sub_me&)
  '
  '  <index&> Index des Fensters in window_array&(index&,x)
  '           Wenn das Objekt aus der MenÅzeile ist: <index&>=-1
  '   <tree&> Index des Objektbaumes
  ' <object&> Index des selektierten Objektes (Åber Mausklick oder Shortcut)
  '     <mc&> Anzahl der Mausklicks (1=normal oder Shortcut / 2=Doppelklick)
  ' <sub_me&> ausgewÑhlter MenÅeintrag in Popup-MenÅs
  '
  SELECT tree&
    '
    ' ------------------------------------------------------------------------
    '
  CASE about&
    SELECT object&
      '
    CASE aboutok&
      @win_close(about_hdle&)
      @mw_redraw
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE mainmenu&
    SELECT object&
      '
    CASE mm_about&
      about_hdle&=@win_open_dialog(2,about&,-1)
    CASE mm_quit&
      exit_program!=TRUE
      '       '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE pre&
    SELECT object&
      '
    CASE pre_info&
      about_hdle&=@win_open_dialog(2,about&,-1)
    CASE pakete&
    CASE man&
    CASE net&
      IF net_var!=TRUE
        @net_config
      ELSE IF net_var!=FALSE
        mintnet_flag!=FALSE
      ENDIF
    CASE devel&
    CASE libs&
    CASE x11&
    CASE preabbr&
      eing&=@cancel_inst
      '
      IF eing&=1
        @win_close(pre_hdle&)
        exit_program!=TRUE
      ENDIF
    CASE preok&
      @pre_auswert
      @win_close(pre_hdle&)
      @mw_redraw
      IF ber_inst!=FALSE
        IF pakete_var!=0
          ALERT 1,alert9_1$,2,alert9_2$,eing&
          IF eing&=1
            @naes_info
          ENDIF
          IF eing&=2
            @install_base
          ENDIF
        ELSE
          @naes_info
        ENDIF
      ELSE IF ber_inst!=TRUE
        IF lstinst_var!=FALSE
          @handle_package_tgz
        ENDIF
        @install_rpm_auto
        IF mintnet_flag!=TRUE
          IF nk_flag!=TRUE
            @netzwerk_config
          ELSE IF nk_flag!=FALSE
            @modem_config
          ENDIF
        ELSE IF mintnet_flag!=FALSE
          exit_program!=TRUE
        ENDIF
      ENDIF
      '       '
      '
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE pfade&
    SELECT object&
      '
    CASE pf_info&
      about_hdle&=@win_open_dialog(2,about&,-1)
      '
    CASE login&
      IF @exist(boot$+":\xaaes\",16)=0
        @rsc_ob_disable(pfade&,naespfad&,FALSE)
        @rsc_ob_draw(index&,naespfad&)
        @rsc_ob_disable(pfade&,naescar&,FALSE)
        @rsc_ob_draw(index&,naescar&)
        @rsc_ob_disable(pfade&,xaaespfad&,FALSE)
        @rsc_ob_draw(index&,xaaespfad&)
      ELSE
        @rsc_ob_disable(pfade&,naespfad&,FALSE)
        @rsc_ob_draw(index&,naespfad&)
        @rsc_ob_disable(pfade&,naescar&,FALSE)
        @rsc_ob_draw(index&,naescar&)
        @rsc_ob_disable(pfade&,xaaespfad&,TRUE)
        @rsc_ob_draw(index&,xaaespfad&)
      ENDIF
    CASE naes_drkt&
      @rsc_ob_disable(pfade&,naespfad&,FALSE)
      @rsc_ob_draw(index&,naespfad&)
      @rsc_ob_disable(pfade&,naescar&,FALSE)
      @rsc_ob_draw(index&,naescar&)
      @rsc_ob_disable(pfade&,xaaespfad&,TRUE)
      @rsc_ob_draw(index&,xaaespfad&)
    CASE xaaesstrt&
      @rsc_ob_disable(pfade&,naespfad&,TRUE)
      @rsc_ob_draw(index&,naespfad&)
      @rsc_ob_disable(pfade&,naescar&,TRUE)
      @rsc_ob_draw(index&,naescar&)
      IF mint115_var&=1
        @rsc_ob_disable(pfade&,xaaespfad&,FALSE)
        @rsc_ob_draw(index&,xaaespfad&)
      ELSE IF mint115_var&=2
        @rsc_ob_disable(pfade&,xaaespfad&,TRUE)
        @rsc_ob_draw(index&,xaaespfad&)
      ENDIF
    CASE plaintos&
      @rsc_ob_disable(pfade&,naespfad&,TRUE)
      @rsc_ob_draw(index&,naespfad&)
      @rsc_ob_disable(pfade&,naescar&,TRUE)
      @rsc_ob_draw(index&,naescar&)
      @rsc_ob_disable(pfade&,xaaespfad&,TRUE)
      @rsc_ob_draw(index&,xaaespfad&)
    CASE naespfad&
      datei$=@fileselector$("NAES.SYS?",boot$+":\*.SYS","",FALSE,"")
      IF @exist(datei$,63)=0
        naespfad_var$=datei$
        @rsc_setup_tree(pfade&)
        @rsc_ob_draw(index&,naespfad&)
      ENDIF
    CASE xaaespfad&
      IF mint115_var&=1
        datei$=@fileselector$("xaaes.tos?",boot$+":\xaaes\","",FALSE,"")
        IF @exist(datei$,63)=0
          xaaespfad_var$=datei$
          @rsc_setup_tree(pfade&)
          @rsc_ob_draw(index&,xaaespfad&)
        ENDIF
        ' ELSE IF mint115_var&=2
        '        xaaespfad_var$=boot$+":\mint\1-16-cur\xaaes\xaloader.prg"
        '        @rsc_setup_tree(pfade&
        '        @rsc_ob_draw(index&,xaaespfad&)
      ENDIF
    CASE pathabbr&
      @win_close(pfade_hdle&)
      exit_program!=TRUE
    CASE pathok&
      @pfade_auswert
      @win_close(pfade_hdle&)
      @mw_redraw
      ' In die MiNT Domain schalten
      ~@pdomain(1)
      '
      @rootfs_install
      @handle_package_tgz
      @install_rpm_auto
      @make_ttytab(start_mode&) ! Hier, da sonst von rpm ÅberbÅgelt
      @mintcnf_create(TRUE)
      '
      IF mintnet_flag!=TRUE
        IF nk_flag!=TRUE
          @netzwerk_config
        ELSE IF nk_flag!=FALSE
          @modem_config
        ENDIF
      ENDIF
      IF mintnet_flag!=FALSE
        @endconf_info
      ENDIF
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE message&
    SELECT object&
      '
    CASE infoabbr&
      eing&=@cancel_inst
      IF eing&=1
        @win_close(mes_hdle&)
        exit_program!=TRUE
        @mw_redraw
      ENDIF
    CASE mes_ok&
      @win_close(mes_hdle&)
      @mw_redraw
      IF info0!=TRUE
        @lnx_suchen
      ENDIF
      '      IF ext2_info!=TRUE
      '      @angemeldete_lw
      '      @lnx_find
      '      ext2_info!=FALSE
      '    ENDIF
      IF boot_info!=TRUE
        @auspacken
        @kernel_copy
        boot_info!=FALSE
      ENDIF
      IF naes_info!=TRUE
        @pfade_vorbeleg
        naes_info!=FALSE
      ENDIF
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE message2&
    SELECT object&
      '
    CASE inf2abbr&
      eing&=@cancel_inst
      IF eing&=1
        @win_close(mes2_hdle&)
        exit_program!=TRUE
        @mw_redraw
      ENDIF
    CASE mes_ok_00&
      @win_close(mes2_hdle&)
      @mw_redraw
      IF neustart_info!=TRUE
        @neustart
        neustart_info!=FALSE
      ENDIF
      IF endconf_info!=TRUE
        @newdesk_auto_del
        exit_program!=TRUE
        ' @kaltstart
        @shutdown
        endconf_info!=FALSE
      ENDIF
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE nic&
    SELECT object&
    CASE nic_info&
      about_hdle&=@win_open_dialog(2,about&,-1)
    CASE kuerzel&
    CASE ipadr&
    CASE netmask&
    CASE dns&
    CASE gateway&
    CASE hn&
    CASE domaine&
    CASE nw_abbr&
      @win_close(nic_hdle&)
    CASE nw_ok&
      @nic_auswert
      '      ALERT 1,alert17_1$+" "+boot$+alert17_2$,1,alert17_3$,eing&
      @nic_driver_copy
      @win_close(nic_hdle&)
      '
      IF ber_inst!=FALSE
        @endconf_info
      ELSE
        exit_program!=TRUE
      ENDIF
      '
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE ppp&
    SELECT object&
    CASE ispname&
    CASE ppp_info&
      about_hdle&=@win_open_dialog(2,about&,-1)
    CASE atz&
    CASE timeout&
    CASE device&
    CASE speed&
    CASE ispnumber&
    CASE user&
    CASE passwd&
      passwd_var$=@invisible_text$(passwd_var$,tree&,window_array&(index&,3),passwd$)
      @rsc_ob_reset(ppp&,passwd&)
    CASE nameserver&
    CASE localip&
    CASE pppdomain&
    CASE remoteip&
    CASE ppp_abbr&
      @win_close(ppp_hdle&)
    CASE ppp_ok&
      @ppp_auswert
      ' ALERT 1," Internetverbindung aus | der bash mit - | pppd call "+ispname_var$+" | - starten!",1," OK ",eing&
      ALERT 1,alert16_1$+" "+ispname_var$+alert16_2$,1,alert16_3$,eing&
      @win_close(ppp_hdle&)
      '
      IF ber_inst!=FALSE
        @endconf_info
      ELSE
        exit_program!=TRUE
      ENDIF
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  CASE preinst&
    SELECT object&
    CASE preinfo&
      about_hdle&=@win_open_dialog(2,about&,-1)
    CASE compi&
      '
    CASE ext2&
    CASE boot&
    CASE xaaes&
      IF xaaes_var!=TRUE
        @redraw_object(preinst&,xaaeszip&,FALSE,preinst_hdle&)
        @redraw_object(preinst&,teradesk&,FALSE,preinst_hdle&)
      ELSE IF xaaes_var!=FALSE
        @rsc_set_text(preinst&,xaaeszip&,"")
        @rsc_set_text(preinst&,tdzip&,"")
        @redraw_object(preinst&,xaaeszip&,TRUE,preinst_hdle&)
        @rsc_ob_state(preinst&,teradesk&,0,FALSE)
        @redraw_object(preinst&,teradesk&,TRUE,preinst_hdle&)
        @redraw_object(preinst&,tdzip&,TRUE,preinst_hdle&)
        teradesk_var!=FALSE
        xaaes_var!=FALSE
      ENDIF
    CASE teradesk&
      IF teradesk_var!=TRUE
        IF @rsc_get_text$(preinst&,xaaeszip&)=""
          ALERT 1,alert19_1$,1,alert19_2$,eing&
          @rsc_ob_state(preinst&,teradesk&,0,FALSE)
          teradesk_var!=FALSE
          @rsc_ob_draw(index&,teradesk&)
          @redraw_object(preinst&,tdzip&,TRUE,preinst_hdle&)
        ELSE
          @redraw_object(preinst&,tdzip&,FALSE,preinst_hdle&)
        ENDIF
      ELSE IF teradesk_var!=FALSE
        @rsc_set_text(preinst&,tdzip&,"")
        @redraw_object(preinst&,tdzip&,TRUE,preinst_hdle&)
      ENDIF
    CASE xaaeszip&
      @xaaeszip_choose
      IF xaaeszip_path$<>""
        IF mint115_var&=1
          IF xaaes_version$<>"963"
            ALERT 1,alert18_1$,1,alert18_2$,eing%
            IF eing%=1
              @set_dateiname(preinst_hdle&,preinst&,xaaeszip&,FALSE)
            ENDIF
          ENDIF
        ENDIF
      ELSE
        @edit_reset(index&,preinst_hdle&,preinst&,xaaes&,xaaeszip&)
      ENDIF
    CASE tdzip&
      @tdeskzip_choose
      IF teradeskzip_path$<>""
        IF mint115_var&=1
          akt_datei$=@dateiname_cut$(teradeskzip_path$)
          IF UPPER$(akt_datei$)<>"TDESK2_3.ZIP"
            ALERT 1,alert21_1$,1,alert21_2$,eing11%
            IF eing11%=1
              @set_dateiname(preinst_hdle&,preinst&,tdzip&,FALSE)
            ENDIF
          ENDIF
        ENDIF
      ELSE
        @edit_reset(index&,preinst_hdle&,preinst&,teradesk&,tdzip&)
      ENDIF
    CASE pabbr&
      eing&=@cancel_inst
      '
      IF eing&=1
        @win_close(preinst_hdle&)
        exit_program!=TRUE
      ENDIF
    CASE pok&
      boot$=CHR$(66+boot_var&)
      lw_fs$=@lower$(CHR$(66+ext2_var&))
      '
      IF xaaes_var!=TRUE AND @rsc_get_text$(preinst&,xaaeszip&)=""
        ALERT 1,alert19_1$,1,alert19_2$,eing&
      ELSE IF teradesk_var!=TRUE AND @rsc_get_text$(preinst&,tdzip&)=""
        ALERT 1,alert20_1$,1,alert20_2$,eing&
      ELSE IF xaaes_var!=FALSE OR (xaaes_var!=TRUE AND @rsc_get_text$(preinst&,xaaeszip&)<>"") OR (xaaes_var!=TRUE AND @rsc_get_text$(preinst&,tdzip&)<>"")
        @win_close(preinst_hdle&)
        @boot_info
        '        @auspacken
      ENDIF
      '
      '
      '  fV-Update: 
      ' Die folgenden Objekte sind durch das Update hinzugekommen:
      '
      '  fV-Update: 
      ' Die folgenden Objekte sind durch das Update weggefallen:
      '     CASE mint115&
      '     CASE mint116&
      '       '  fV-Update 
      '       '
      '  fV-Update 
      '
    ENDSELECT
    '
    ' ------------------------------------------------------------------------
    '
  ENDSELECT
RETURN
